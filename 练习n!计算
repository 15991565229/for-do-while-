#include<stdio.h>
int main
{
    int i =0;
    int n =0;
    int ret = 1;
    scanf("%d\n",n);
    for(i = 1;i<=n;i++)
    {
       ret = ret*i;
    }
    printf("ret=%d\n",ret);
    return 0;
 }
 
 计算1！+2！+3！
 #include<stdio.h>
int main
{
    int i =0;
    int n =0;
    int ret = 1;
    int sum = 0;
    for(n= 1;n<=3;n++)
    {
        ret = 1;
        for(i = 1;i<=n;i++)
        {
            ret = ret*i;
        }
        sun = sum +ret;
    }
    printf("ret=%d\n",ret);
    return 0;
 }
